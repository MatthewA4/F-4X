    <!-- Reference: ADA101648, page 25 for positive AoA including 0-->
    <function name="aero/coefficient/CL/CL_BASIC">
      <description>CL_BASIC</description>
      <product>
          <property>aero/qbar-psf</property>
          <property>metrics/Sw-sqft</property>
          <sum>
            <ifthen>
              <le> <p> aero/alpha-deg </p> <v> 0 </v> </le>
              <product>
                <v> 0.122 </v><!-- CL_basic at 0 deg alpha -->
